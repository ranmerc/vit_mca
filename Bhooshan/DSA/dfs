#include<iostream>
#include<vector>

using namespace std;

void dfs(int i, vector<vector<int>> &adj, vector<int>&visited){
        cout<<i<<" ";
        visited[i] = 1;
        for(auto it : adj[i]){
            if(!visited[i]) dfs(it,adj,visited);
        }
}

void dfsGraph(vector<vector<int>> &adj, int v)
{
    vector<int>visited(v,0);
    for(int i = 0; i < v; i++){
        if(!visited[i]) dfs(i,adj,visited);
    }
}

int main(){
    int V,e;
    cin>>V>>e;

    int u,v;
    vector<vector<int>> adj(V);

    for(int i = 0; i < e; i++){
        cin>>u>>v;
        adj[u].push_back(v);
    }

    dfsGraph(adj,V);


    return 0;
}
